<h1>Alle løb</h1>

<table class="pretty">
  <tr>
    <th>Navn</th>
    <th colspan="2"></th>
  </tr>
<% @loebs.each do |loeb| %>
  <tr>
    <td><%= loeb.name %></td>
    <td><%= link_to 'Vis alle poster', loeb %></td>
    <td><%= link_to 'Print poster', loeb_path(loeb, :print=>true) %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'Nyt løb', new_loeb_path, :class => 'button' %>
