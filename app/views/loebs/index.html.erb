<h1>Listing loebs</h1>

<table>
  <tr>
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @loebs.each do |loeb| %>
  <tr>
    <td><%= loeb.name %></td>
    <td><%= link_to 'Show', loeb %></td>
    <td><%= link_to 'Edit', edit_loeb_path(loeb) %></td>
    <td><%= link_to 'Destroy', loeb, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

